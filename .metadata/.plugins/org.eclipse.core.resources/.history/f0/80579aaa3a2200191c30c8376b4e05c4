import java.time.LocalDate;
import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;


public class Lopper {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		List n1 = new ArrayList();
		
		System.out.println("Printing current time here");
		LocalDate today = LocalDate.now();
		System.out.println("date is " + today);

		Scanner scanner = new Scanner(System.in);
		System.out.println("Enter the number to check if it a prime or not");
		int n = scanner.nextInt();

		// Isprime check = new Isprime();
		// check.isPrime(n);

		System.out.println("Printing a number traingle");
		for (int i = 1; i <= 5; i++) {
			for (int j = 1; j <= i; j++) {
				System.out.print(j);
			}
			System.out.println("\n");
		}

	}

}
