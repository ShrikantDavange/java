import java.util.Scanner;

public class Lopper {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scanner = new Scanner(System.in);
		System.out.println("Enter the number to check if it a prime or not");
		int n = scanner.nextInt();

		// Isprime check = new Isprime();
		// check.isPrime(n);

		System.out.println("Printing a number traingle");
		for (int i = 1; i <= 5; i++) {
			for (int j = 1; j <= i; j++) {
				System.out.print(j);
			}

		}

	}

}
